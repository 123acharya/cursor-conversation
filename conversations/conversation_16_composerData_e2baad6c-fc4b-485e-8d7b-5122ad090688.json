      "file:///c%3A/Users/pc/Desktop/sentence_typer.py": {
        "content": "import time\nimport threading\nfrom pynput.keyboard import Controller\nimport tkinter as tk\nfrom tkinter import scrolledtext, messagebox\n\n# Initialize the keyboard controller\nkeyboard = Controller()\n\n# Initialize typing speed (delay between characters in seconds)\ntyping_speed = 0.05  # Default: 0.05 seconds between characters\n\n# Function to type a sentence\ndef type_sentence(sentence):\n    \"\"\"Types the given sentence character by character\"\"\"\n    global typing_speed\n    for char in sentence:\n        keyboard.type(char)\n        time.sleep(typing_speed)  # Delay between characters\n\n# Function to handle the submit action\ndef submit_sentence():\n    sentence = sentence_entry.get(\"1.0\", tk.END).strip()\n    \n    if not sentence:\n        messagebox.showwarning(\"Warning\", \"Please enter a sentence to type.\")\n        return\n    \n    # Get delay time\n    try:\n        delay = int(delay_entry.get().strip())\n        if delay < 0:\n            raise ValueError(\"Delay must be a positive number\")\n    except ValueError:\n        messagebox.showerror(\"Error\", \"Please enter a valid delay time (in seconds).\")\n        return\n    \n    # Disable the submit button during countdown\n    submit_button.config(state='disabled')\n    \n    # Update status\n    status_label.config(text=f\"Starting in {delay}s...\", fg=\"orange\")\n    root.update()\n    \n    # Run the typing sequence in a separate thread\n    typing_thread = threading.Thread(target=countdown_and_type, args=(sentence, delay))\n    typing_thread.daemon = True\n    typing_thread.start()\n\ndef countdown_and_type(sentence, delay):\n    \"\"\"Countdown for specified seconds, then type the sentence\"\"\"\n    # Countdown from delay to 1\n    for remaining in range(delay, 0, -1):\n        status_label.config(text=f\"Starting in {remaining}s...\", fg=\"orange\")\n        root.update()\n        time.sleep(1)\n    \n    # Final message\n    status_label.config(text=\"Typing...\", fg=\"green\")\n    root.update()\n    \n    # Type the sentence\n    time.sleep(0.5)  # Small delay before typing starts\n    type_sentence(sentence)\n    \n    # Update status\n    status_label.config(text=\"Done!\", fg=\"blue\")\n    root.update()\n    \n    # Re-enable the submit button\n    submit_button.config(state='normal')\n\n# Function to clear the sentence field\ndef clear_sentence():\n    sentence_entry.delete(\"1.0\", tk.END)\n\n# Function to increase typing speed (decrease delay)\ndef increase_speed():\n    global typing_speed\n    if typing_speed > 0.01:  # Minimum delay of 0.01 seconds\n        typing_speed -= 0.01\n        typing_speed = round(typing_speed, 2)  # Round to 2 decimal places\n        update_speed_display()\n\n# Function to decrease typing speed (increase delay)\ndef decrease_speed():\n    global typing_speed\n    if typing_speed < 1.0:  # Maximum delay of 1.0 seconds\n        typing_speed += 0.01\n        typing_speed = round(typing_speed, 2)  # Round to 2 decimal places\n        update_speed_display()\n\n# Function to update the speed display label\ndef update_speed_display():\n    global typing_speed\n    cps = round(1.0 / typing_speed, 1) if typing_speed > 0 else 0\n    speed_display_label.config(text=f\"{cps} chars/sec\")\n\n# Create the main window\nroot = tk.Tk()\nroot.title(\"Sentence Typer\")\nroot.geometry(\"500x350\")\n\n# Create main frame\nmain_frame = tk.Frame(root, padx=15, pady=15)\nmain_frame.pack(fill=tk.BOTH, expand=True)\n\n# Controls frame - compact layout\ncontrols_frame = tk.Frame(main_frame)\ncontrols_frame.pack(pady=(0, 8))\n\n# Delay\ntk.Label(controls_frame, text=\"Delay:\", font=(\"Arial\", 10)).pack(side=tk.LEFT, padx=(0, 5))\ndelay_entry = tk.Entry(controls_frame, font=(\"Arial\", 10), width=5, justify='center')\ndelay_entry.pack(side=tk.LEFT, padx=(0, 15))\ndelay_entry.insert(0, \"4\")\n\n# Speed\ntk.Label(controls_frame, text=\"Speed:\", font=(\"Arial\", 10)).pack(side=tk.LEFT, padx=(0, 5))\ndecrease_speed_button = tk.Button(controls_frame, text=\"âˆ’\", command=decrease_speed, font=(\"Arial\", 10), bg=\"lightcoral\", fg=\"white\", width=2)\ndecrease_speed_button.pack(side=tk.LEFT, padx=2)\nspeed_display_label = tk.Label(controls_frame, text=\"20.0\", font=(\"Arial\", 10), fg=\"blue\", width=6)\nspeed_display_label.pack(side=tk.LEFT, padx=2)\nincrease_speed_button = tk.Button(controls_frame, text=\"+\", command=increase_speed, font=(\"Arial\", 10), bg=\"lightgreen\", fg=\"white\", width=2)\nincrease_speed_button.pack(side=tk.LEFT, padx=2)\n\n# Sentence entry field\nsentence_entry = scrolledtext.ScrolledText(main_frame, wrap=tk.WORD, width=55, height=6, font=(\"Consolas\", 10))\nsentence_entry.pack(pady=(0, 8), fill=tk.BOTH, expand=True)\nsentence_entry.focus()\n\n# Buttons and status frame\nbottom_frame = tk.Frame(main_frame)\nbottom_frame.pack(pady=(5, 0))\n\nsubmit_button = tk.Button(bottom_frame, text=\"Submit\", command=submit_sentence, font=(\"Arial\", 11), bg=\"green\", fg=\"white\", padx=15, pady=3)\nsubmit_button.pack(side=tk.LEFT, padx=5)\n\nclear_button = tk.Button(bottom_frame, text=\"Clear\", command=clear_sentence, font=(\"Arial\", 10), bg=\"lightgray\", fg=\"black\", padx=10, pady=3)\nclear_button.pack(side=tk.LEFT, padx=5)\n\nstatus_label = tk.Label(bottom_frame, text=\"Ready\", font=(\"Arial\", 10), fg=\"gray\")\nstatus_label.pack(side=tk.LEFT, padx=15)\n\n# Initialize speed display\nupdate_speed_display()\n\n# Start the GUI loop\nroot.mainloop()\n\n",
